{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prisma-generate": "dotenv -e .env.production.local -- pnpm exec prisma generate",
    "prisma-generate-dev": "dotenv -e .env.development.local -- pnpm exec prisma generate",
    "prisma-seed-dev": "pnpm exec prisma db seed",
    "prisma-db-push": "dotenv -e .env.development.local -- pnpm exec prisma db push",
    "prisma-migrate-prod": "dotenv -e .env.production.local -- pnpm exec prisma migrate deploy",
    "prisma-migrate-dev": "dotenv -e .env.development.local -- pnpm exec prisma migrate dev",
    "prisma-migrate-reset": "dotenv -e .env.development.local -- pnpm exec prisma migrate reset --force",
    "prisma-migrate-ci": "pnpm exec prisma migrate reset --force && pnpm exec prisma generate",
    "build": "tsc --project tsconfig.build.json",
    "start-prod": "dotenv -e .env.development.local -- pnpm run build && dotenv -e .env.production.local -- node dist/index.js",
    "start-dev": "dotenv -e .env.development.local -- pnpm run build && dotenv -e .env.development.local -- nodemon",
    "start": "pnpm run build && node dist/index.js",
    "start:nodemon": "nodemon",
    "test": "jest --colors --detectOpenHandles --coverage --testPathIgnorePatterns=src/__tests__/e2e/*",
    "test-e2e": "jest --colors --runInBand ./src/__tests__/e2e/tests "
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.276.0",
    "@casl/ability": "6.3.1",
    "@casl/prisma": "1.3.0",
    "@fast-csv/parse": "^4.3.6",
    "@prisma/client": "4.8.1",
    "@types/bcryptjs": "^2.4.4",
    "@types/multer": "^1.4.7",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-cli": "^6.0.0",
    "email-validator": "^2.0.4",
    "express": "^4.18.1",
    "fast-csv": "^4.3.6",
    "grammy": "^1.20.4",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "node-mocks-http": "^1.11.0",
    "prisma": "4.8.1",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/selenium-webdriver": "^4.1.5",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "nodemon": "^2.0.20",
    "selenium-webdriver": "^4.4.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
