{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prisma-generate": "dotenv -e .env.production.local -- npx prisma generate",
    "prisma-migrate-prod": "dotenv -e .env.production.local -- npx prisma migrate deploy",
    "prisma-migrate-dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "prisma-migrate-ci": "npx prisma migrate reset --force && npx prisma migrate dev --name init",
    "build": "tsc --project tsconfig.build.json",
    "start-prod": "npm run build && dotenv -e .env.production.local -- node dist/index.js",
    "start-dev": "npm run build && dotenv -e .env.development.local -- node dist/index.js",
    "start": "npm run build && node dist/index.js",
    "test": "jest --colors --detectOpenHandles --coverage --testPathIgnorePatterns=src/__tests__/e2e/*",
    "test-e2e": "jest --colors --runInBand ./src/__tests__/e2e "
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "node-mocks-http": "^1.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "prisma": "^4.2.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
